namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int image = 1; //пусть в качестве картинки будет выступать цифра 1
            float countSlot;//кол-во слотов/стэков
            int imageRemains;
            Console.WriteLine("сколько должно быть картинок в слоте?");
            int imageInSlot = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("сколько должно быть всего картинок?");
            int imageAll = Convert.ToInt32(Console.ReadLine());
            countSlot = imageAll / imageInSlot;
            imageRemains = imageAll % imageInSlot;
            Console.WriteLine("полностью заполненных рядов: "+countSlot);
            Console.WriteLine("оставшихся картинок: " + imageRemains);
        }
    }
}
