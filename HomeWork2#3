static void Main(string[] args)
        {
            int step = 7;
            int borderCycle = 100;
            int variableStart = 5;

            for (; variableStart < borderCycle; variableStart += step) // простое перечисление цифр от 5 до 96 (поскольку, есть последовательность(каждое число больше другого на 7), можно использовать как for, так и while)
            {
                Console.WriteLine(variableStart);
            }
        } 
